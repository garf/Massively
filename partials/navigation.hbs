<nav id="nav">
    <ul class="links">
        <li class="nav-{{slug}}"><a href="{{@site.url}}"><img src="{{@site.logo}}" alt="{{@site.title}}" style="height: 100%;" /></a></li>
        {{#foreach navigation}}
        <li class="nav-{{slug}} {{#if current}}active{{/if}}"><a href="{{url absolute="true"}}">{{label}}</a></li>
        {{/foreach}}
    </ul>
    <ul class="icons">
        <li><a href="https://t.me/atheist_mind" class="icon fa-telegram" title="Telegram"><span class="label">Telegram</span></a></li>
        {{#if @site.twitter}}
        <li><a href="{{twitter_url @site.twitter}}" class="icon fa-twitter" target="_blank" title="Twitter"><span class="label">Twitter</span></a></li>
        {{/if}}
        {{#if @site.facebook}}
        <li><a href="{{facebook_url @site.facebook}}" class="icon fa-facebook" target="_blank" title="Facebook"><span class="label">Facebook</span></a></li>
        {{/if}}
        <li><a href="https://www.instagram.com/atheist_mind_ig/" class="icon fa-instagram" title="Instagram"><span class="label">Instagram</span></a></li>
        <li><a href="https://feedly.com/i/subscription/feed/{{@site.url}}/rss/" class="icon fa-rss" target="_blank" rel="noopener"><span class="label" title="RSS">RSS</span></a></li>
    </ul>
</nav>
